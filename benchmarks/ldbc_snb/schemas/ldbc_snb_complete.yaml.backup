# COMPLETE LDBC SNB Interactive v1 Schema for ClickGraph
# Maps all LDBC tables to graph model for comprehensive benchmark testing

name: ldbc_snb_complete

graph_schema:
  nodes:
    # Person - central node type
    - label: Person
      database: ldbc
      table: Person
      node_id: id
      property_mappings:
        id: id
        firstName: firstName
        lastName: lastName
        gender: gender
        birthday: birthday
        creationDate: creationDate
        locationIP: locationIP
        browserUsed: browserUsed

    # Post - content in forums
    - label: Post
      database: ldbc
      table: Post
      node_id: id
      property_mappings:
        id: id
        imageFile: imageFile
        creationDate: creationDate
        locationIP: locationIP
        browserUsed: browserUsed
        language: language
        content: content
        length: length

    # Comment - replies to posts/comments
    - label: Comment
      database: ldbc
      table: Comment
      node_id: id
      property_mappings:
        id: id
        creationDate: creationDate
        locationIP: locationIP
        browserUsed: browserUsed
        content: content
        length: length

    # Message - unified Post + Comment
    - label: Message
      database: ldbc
      table: Message
      node_id: id
      property_mappings:
        id: id
        creationDate: creationDate
        locationIP: locationIP
        browserUsed: browserUsed
        content: content
        length: length

    # Forum - container for posts
    - label: Forum
      database: ldbc
      table: Forum
      node_id: id
      property_mappings:
        id: id
        title: title
        creationDate: creationDate

    # Tag - content categorization
    - label: Tag
      database: ldbc
      table: Tag
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

    # TagClass - tag classification
    - label: TagClass
      database: ldbc
      table: TagClass
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

    # Place - geographic location
    - label: Place
      database: ldbc
      table: Place
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url
        type: type

    # City - Place type filter
    - label: City
      database: ldbc
      table: Place
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

    # Country - Place type filter
    - label: Country
      database: ldbc
      table: Place
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

    # Organisation - universities and companies
    - label: Organisation
      database: ldbc
      table: Organisation
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url
        type: type

    # Company - Organisation type filter
    - label: Company
      database: ldbc
      table: Organisation
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

    # University - Organisation type filter
    - label: University
      database: ldbc
      table: Organisation
      node_id: id
      property_mappings:
        id: id
        name: name
        url: url

  edges:
    # Person relationships
    - type: KNOWS
      database: ldbc
      table: Person_knows_Person
      from_id: Person1Id
      to_id: Person2Id
      from_node: Person
      to_node: Person
      property_mappings:
        creationDate: creationDate

    - type: IS_LOCATED_IN
      database: ldbc
      table: Person_isLocatedIn_Place
      from_id: PersonId
      to_id: CityId
      from_node: Person
      to_node: Place
      property_mappings:
        creationDate: creationDate

    - type: HAS_INTEREST
      database: ldbc
      table: Person_hasInterest_Tag
      from_id: PersonId
      to_id: TagId
      from_node: Person
      to_node: Tag
      property_mappings:
        creationDate: creationDate

    - type: STUDY_AT
      database: ldbc
      table: Person_studyAt_Organisation
      from_id: PersonId
      to_id: UniversityId
      from_node: Person
      to_node: Organisation
      property_mappings:
        creationDate: creationDate
        classYear: classYear

    - type: WORK_AT
      database: ldbc
      table: Person_workAt_Organisation
      from_id: PersonId
      to_id: CompanyId
      from_node: Person
      to_node: Organisation
      property_mappings:
        workFrom: workFrom

    # Post relationships
    - type: HAS_CREATOR
      database: ldbc
      table: Post_hasCreator_Person
      from_id: PostId
      to_id: PersonId
      from_node: Post
      to_node: Person
      property_mappings:
        creationDate: creationDate

    - type: POST_LOCATED_IN
      database: ldbc
      table: Post_isLocatedIn_Place
      from_id: PostId
      to_id: CountryId
      from_node: Post
      to_node: Place
      property_mappings:
        creationDate: creationDate

    - type: HAS_TAG
      database: ldbc
      table: Post_hasTag_Tag
      from_id: PostId
      to_id: TagId
      from_node: Post
      to_node: Tag
      property_mappings:
        creationDate: creationDate

    # Comment relationships
    - type: COMMENT_HAS_CREATOR
      database: ldbc
      table: Comment_hasCreator_Person
      from_id: CommentId
      to_id: PersonId
      from_node: Comment
      to_node: Person
      property_mappings:
        creationDate: creationDate

    - type: COMMENT_LOCATED_IN
      database: ldbc
      table: Comment_isLocatedIn_Place
      from_id: CommentId
      to_id: CountryId
      from_node: Comment
      to_node: Place
      property_mappings:
        creationDate: creationDate

    - type: COMMENT_HAS_TAG
      database: ldbc
      table: Comment_hasTag_Tag
      from_id: CommentId
      to_id: TagId
      from_node: Comment
      to_node: Tag
      property_mappings:
        creationDate: creationDate

    - type: REPLY_OF_POST
      database: ldbc
      table: Comment_replyOf_Post
      from_id: CommentId
      to_id: PostId
      from_node: Comment
      to_node: Post
      property_mappings:
        creationDate: creationDate

    - type: REPLY_OF_COMMENT
      database: ldbc
      table: Comment_replyOf_Comment
      from_id: Comment1Id
      to_id: Comment2Id
      from_node: Comment
      to_node: Comment
      property_mappings:
        creationDate: creationDate

    # Message unified relationships
    - type: MESSAGE_HAS_CREATOR
      database: ldbc
      table: Message_hId
      to_id: PersonId
      from_node: Message
      to_node: Person
      property_mappings:
        creationDate: creationDate
      property_mappings: {}

    # Person likes
    - type: LIKES_POST
      database: ldbc
      table: Person_lId
      to_id: PostIon_id
      to_id: Post_id
      from_node: Person
      to_node: Post
      property_mappings:
        creationDate: creationDate

    - type: LIKES_COMMENT
      database: ldbc
      table: Person_lId
      to_id: CommentIid
      to_id: Comment_id
      from_node: Person
      to_node: Comment
      property_mappings:
        creationDate: creationDate

    - type: LIKES
      database: ldbc
      table: Person_lId
      to_id: MessageIid
      to_id: Message_id
      from_node: Person
      to_node: Message
      property_mappings:
        creationDate: creationDate

    # Forum relationships
    - type: CONTAINER_OF
      database: ldbc
      table: Forum_cId
      to_id: PostId
      from_node: Forum
      to_node: Post
      property_mappings:
        creationDate: creationDate
      property_mappings: {}

    - type: HAS_MODERATOR
      database: ldbc
      table: Forum_hId
      to_id: PersonId
      from_node: Forum
      to_node: Person
      property_mappings:
        creationDate: creationDate
      property_mappings: {}

    - type: HAS_MEMBER
      database: ldbc
      table: Forum_hId
      to_id: PersonId
      from_node: Forum
      to_node: Person
      property_mappings:
        creationDate: creationDate
      property_mappings:
        joinDate: joinDate

    - type: FORUM_HAS_TAG
      database: ldbc
      table: Forum_hId
      to_id: TagId
      from_node: Forum
      to_node: Tag
      property_mappings:
        creationDate: creationDate
      property_mappings: {}

    # Tag/OrganisaId
      to_id: TagClassI
      database: ldbc
      table: Tag_hasType_TagClass
      from_id: Tag_id
      to_id: TagClass_id
      from_node: Tag
      to_node: TagClass
      property_mappings: {}
1Id
